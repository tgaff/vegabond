<div class=".col-md-4 col-md-offset-2">

  <div class="profile-show">
    <h1><%= @post.title%></h1>
    <%= @post.city%> <br><br><br>
    <p><%= @post.story%></p>
  </div>
  <%= button_to "Delete", @post,:class => "btn btn-danger", method: :delete %>

  <%= link_to @post.city, city_path(@post.city_id) %>

</div>
